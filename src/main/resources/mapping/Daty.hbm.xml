<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mapping.Daty" table="daty" schema="public" catalog="taxi">
        <id name="dataId">
            <column name="data_id" sql-type="integer"/>
        </id>
        <property name="dzien">
            <column name="dzien" sql-type="integer"/>
        </property>
        <property name="miesiac">
            <column name="miesiac" sql-type="integer"/>
        </property>
        <property name="rok">
            <column name="rok" sql-type="integer"/>
        </property>
        <set name="kursiesByDataId" inverse="true">
            <key>
                <column name="data_id"/>
            </key>
            <one-to-many not-found="ignore" class="mapping.Kursy"/>
        </set>
    </class>
</hibernate-mapping>